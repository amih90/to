{
  "name": "to",
  "displayName": "To",
  "version": "0.0.11",
  "publisher": "amih90",
  "author": {
    "name": "Ami Hollander",
    "email": "amih90@gmail.com"
  },
  "description": "Encode, decode, format and convert everything",
  "keywords": [
    "encode",
    "decode",
    "format",
    "convert",
    "utilities"
  ],
  "categories": [
    "Other"
  ],
  "homepage": "https://github.com/amih90/to",
  "repository": {
    "type": "git",
    "url": "https://github.com/amih90/to.git"
  },
  "engines": {
    "vscode": "^1.47.0"
  },
  "activationEvents": [
    "*",
    "onCommand:to.base32",
    "onCommand:to.base64",
    "onCommand:to.binary",
    "onCommand:to.hex",
    "onCommand:to.urlencode"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Enable CodeLens",
        "command": "to.enableCodeLens",
        "category": "To"
      },
      {
        "title": "Disable Codelens",
        "command": "to.disableCodeLens",
        "category": "To"
      },
      {
        "command": "to.base32",
        "title": "Base32",
        "category": "To"
      },
      {
        "command": "to.base64",
        "title": "Base64",
        "category": "To"
      },
      {
        "command": "to.binary",
        "title": "Binary",
        "category": "To"
      },
      {
        "command": "to.hex",
        "title": "Hex",
        "category": "To"
      },
      {
        "command": "to.urlencode",
        "title": "Urlencode",
        "category": "To"
      },
      {
        "command": "to.string.reverse",
        "title": "String.reverse",
        "category": "To"
      },
      {
        "command": "to.string.lowerCase",
        "title": "String.lowercase",
        "category": "To"
      },
      {
        "command": "to.string.upperCase",
        "title": "String.UPPERCASE",
        "category": "To"
      },
      {
        "command": "to.string.snakeCase",
        "title": "String.snake_case",
        "category": "To"
      },
      {
        "command": "to.string.dotCase",
        "title": "String.dot.case",
        "category": "To"
      },
      {
        "command": "to.string.whiteSpace",
        "title": "String.white__space",
        "category": "To"
      },
      {
        "command": "to.string.camelCase",
        "title": "String.camelCase",
        "category": "To"
      },
      {
        "command": "to.string.kebabCase",
        "title": "String.kebab-case",
        "category": "To"
      },
      {
        "command": "to.string.deburr",
        "title": "String.deburr",
        "category": "To"
      },
      {
        "command": "to.string.trim",
        "title": "String.trim",
        "category": "To"
      },
      {
        "command": "to.string.pascalCase",
        "title": "String.PascalCase",
        "category": "To"
      },
      {
        "command": "to.string.allCaps",
        "title": "String.ALL_CAPS",
        "category": "To"
      }
    ],
    "configuration": {
      "properties": {
        "to.enableCodeLens": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "clean": "rm -rf out || rmdir out /s /q",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "package": "npm run clean && vsce package",
    "package-and-install": "npm run package && node ./.vscode/install-package.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.47.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^10.2.0",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@ctrl/ts-base32": "^1.2.2",
    "@types/flatbuffers": "^1.10.0",
    "@types/lodash": "^4.14.158",
    "flatbuffers": "^1.12.0",
    "lodash": "^4.17.21"
  }
}